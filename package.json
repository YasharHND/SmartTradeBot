{
  "name": "aws-stack",
  "version": "1.0.0",
  "scripts": {
    "format": "prettier --write .",
    "check:format": "prettier --check .",
    "lint": "eslint --flag unstable_config_lookup_from_file --fix .",
    "check:lint": "eslint --flag unstable_config_lookup_from_file .",
    "build": "tsc",
    "infra:deploy:backend": "cdk --app 'ts-node --prefer-ts-exts ./infra/backend.app.ts' deploy",
    "infra:destroy": "pnpm run infra:destroy:backend",
    "infra:destroy:backend": "cdk --app 'ts-node --prefer-ts-exts ./infra/backend.app.ts' destroy",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.3.0",
    "aws-cdk": "^2.1027.0",
    "esbuild": "^0.25.9",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.25.2",
    "@aws-sdk/client-cloudformation": "^3.876.0",
    "aws-cdk-lib": "^2.213.0",
    "axios": "^1.11.0",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  }
}
