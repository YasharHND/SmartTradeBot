{
  "name": "aws-stack",
  "version": "1.0.0",
  "scripts": {
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "prebuild": "tsc --noEmit",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "infra:deploy:backend": "dotenv -e .env -- cdk --app 'ts-node --prefer-ts-exts --project tsconfig.json -r tsconfig-paths/register ./infra/backend.app.ts' deploy --require-approval never",
    "infra:destroy": "pnpm run infra:destroy:backend",
    "infra:destroy:backend": "dotenv -e .env -- cdk --app 'ts-node --prefer-ts-exts --project tsconfig.json -r tsconfig-paths/register ./infra/backend.app.ts' destroy --require-approval never",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "aws-cdk": "^2.1033.0",
    "dotenv-cli": "^11.0.0",
    "esbuild": "^0.27.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.946.0",
    "aws-cdk-lib": "^2.232.0",
    "axios": "^1.13.2",
    "constructs": "^10.4.3",
    "source-map-support": "^0.5.21"
  }
}
